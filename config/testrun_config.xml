<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Selenium Demo" verbose="1" thread-count="2" parallel="tests">

	<listeners>
      	<listener class-name="org.uncommons.reportng.HTMLReporter"/>
      	<listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
  	</listeners>
  	
  	<parameter name="browser" value="firefoxDriver"></parameter>
  	
	<test name="Selenium Demo" enabled="true">
		<classes>
			<class name="com.automation.test.SeleniumDemoTestSuite">
			</class>
		</classes>
	</test>

</suite>